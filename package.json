{
  "name": "renovations-ng",
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "ng": "ng",
    "build": "node --max_old_space_size=5048 ./node_modules/@angular/cli/bin/ng build",
    "bs": "ng build && npm run start",
    "bs:pwa": "ng build --prod && npm run http-serve",
    "start": "node server.js",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "serve:ssr:renovations-ng": "node dist/renovations-ng/server/server.mjs",
    "lint": "ng lint"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^19.2.10",
    "@angular/cdk": "^19.2.15",
    "@angular/common": "^19.2.10",
    "@angular/compiler": "^19.2.10",
    "@angular/core": "^19.2.10",
    "@angular/forms": "^19.2.10",
    "@angular/material": "^19.2.15",
    "@angular/platform-browser": "^19.2.10",
    "@angular/platform-browser-dynamic": "^19.2.10",
    "@angular/platform-server": "^19.2.10",
    "@angular/router": "^19.2.10",
    "@angular/ssr": "^19.2.11",
    "@ngx-translate/core": "^16.0.4",
    "@ngx-translate/http-loader": "^16.0.1",
    "@sentry/angular": "^9.35.0",
    "express": "^5.1.0",
    "idb": "^8.0.3",
    "rxjs": "~7.8.0",
    "rxjs-interop": "^2.0.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.15.0"
